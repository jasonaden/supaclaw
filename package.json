{
  "name": "openclaw-memory",
  "version": "1.0.0",
  "description": "Persistent memory for AI agents using Supabase - sessions, memories, semantic search",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "openclaw-memory": "./dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "migrations/**/*",
