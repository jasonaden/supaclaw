{
  "name": "supaclaw",
  "version": "1.0.0",
  "description": "Persistent memory for AI agents using Supabase - sessions, memories, semantic search",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    },
    "./errors": {
      "types": "./dist/error-handling.d.ts",
      "require": "./dist/error-handling.js"
    },
    "./context": {
      "types": "./dist/context-manager.d.ts",
      "require": "./dist/context-manager.js"
    }
  },
  "bin": {
    "supaclaw": "dist/cli.js"
  },
  "files": [
    "dist/**/*",
    "migrations/**/*",
    "docs/guides/**",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "SCHEMA.md"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "agents",
    "memory",
    "supabase",
    "postgres",
    "vector-search",
    "semantic-search",
    "llm",
    "context-management",
    "supaclaw",
    "clawdbot",
    "ai-memory",
    "conversation-history",
    "chat-history",
    "context-window",
    "rag"
  ],
  "author": "Han Kim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Arephan/supaclaw.git"
  },
  "bugs": {
    "url": "https://github.com/Arephan/supaclaw/issues"
  },
  "homepage": "https://github.com/Arephan/supaclaw#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "commander": "^14.0.3",
    "openai": "^6.17.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
