  "author": "Han Kim",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Arephan/openclaw-memory"
  },
  "bugs": {
    "url": "https://github.com/Arephan/openclaw-memory/issues"
  },
  "homepage": "https://github.com/Arephan/openclaw-memory#readme",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "commander": "^14.0.3",
    "openai": "^6.17.0"
  },
  "devDependencies": {
    "@types/commander": "^2.12.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
